    "%%time\n",
    "#openpyxlを導入\n",
    "import openpyxl\n",
    "wb_im=openpyxl.load_workbook(file+im)\n",
    "\n",
    "sheet_im=wb_im['Sheet1']\n",
    "\n",
    "#pandasを導入\n",
    "import pandas as pd\n",
    "data1=pd.read_csv(file+data, usecols=[0], skiprows=[0], header=None)[0]\n",
    "data2=pd.read_csv(file+data, usecols=[1], skiprows=[0], header=None)[1] \n",
    "\n",
    "\n",
    "#ここから本番\n",
    "#R(L)2用　　R2_finまで\n",
    "wb_ex1=openpyxl.load_workbook(file+ex1)\n",
    "i=1\n",
    "j=1\n",
    "g=1\n",
    "while True:\n",
    "    if (sheet_im.cell(row=i,column=1).value)==None or \\\n",
    "       (sheet_im.cell(row=i,column=1).value)>R2_fin:\n",
    "        break\n",
    "    else:\n",
    "        a1=int((sheet_im.cell(row=i,column=1).value)/0.00004)\n",
    "        a2=int((sheet_im.cell(row=i+1,column=1).value)/0.00004)\n",
    "        sheet_ex1=wb_ex1['Sheet'+str(g)]\n",
    "        i=i+2\n",
    "        g=g+1\n",
    "        sheet_ex1.cell(row=1,column=1).value='Time'\n",
    "        sheet_ex1.cell(row=1,column=2).value='Volt'\n",
    "        s=2\n",
    "        while a1<a2:\n",
    "              sheet_ex1.cell(row=s,column=1).value=data1[a1]\n",
    "              sheet_ex1.cell(row=s,column=2).value=data2[a1]\n",
    "              s=s+1\n",
    "              a1=a1+1\n",
    "   \n",
    "wb_ex1.save(file+ex1)\n",
    "\n",
    "\n",
